		"jest": "29.4.3",
		"jest-environment-jsdom": "29.4.3",
		"lint-staged": "^14.0.1",
		"nx": "16.3.1",
		"prettier": "^2.8.7",
		"ts-jest": "29.1.0",
		"ts-node": "10.9.1",
		"typedoc": "^0.24.8",
		"typedoc-plugin-markdown": "^3.15.3",
		"typescript": "5.0.4"
	},
	"pnpm": {
		"overrides": {
			"trim@<0.0.3": ">=0.0.3",
			"got@<11.8.5": ">=11.8.5"
		}
