{
	"name": "socketdb",
	"version": "8.2.0",
	"description": "Real-time synchronized data storage",
	"homepage": "https://socketdb.com",
	"license": "MIT",
	"scripts": {
		"prepare": "is-ci || husky install",
		"docs": "pnpm nx serve docs"
