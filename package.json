		"*": [
			"nx format:write --files"
		]
	},
	"dependencies": {
		"krog": "^1.1.0",
		"tslib": "^2.3.0",
		"ws": "^7.5.9"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@nrwl/cli": "15.0.13",
		"@nrwl/devkit": "^15.0.13",
		"@nrwl/eslint-plugin-nx": "15.0.13",
		"@nrwl/jest": "15.0.13",
		"@nrwl/js": "15.0.13",
		"@nrwl/linter": "15.0.13",
		"@nrwl/rollup": "15.0.13",
		"@nrwl/workspace": "15.0.13",
		"@types/jest": "28.1.1",
		"@types/node": "18.7.1",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"cz-git": "^1.3.12",
		"eslint": "~8.15.0",
		"eslint-config-prettier": "8.1.0",
		"husky": "^8.0.0",
		"is-ci": "^3.0.1",
		"jest": "28.1.1",
		"jest-environment-jsdom": "28.1.1",
		"lint-staged": "^13.0.3",
		"nx": "15.0.13",
		"prettier": "^2.6.2",
		"ts-jest": "28.0.5",
		"ts-node": "10.9.1",
		"typescript": "~4.8.2",
		"vuepress": "^1.9.7",
		"vuepress-plugin-check-md": "^0.0.2",
		"vuepress-plugin-one-click-copy": "^1.0.6",
		"vuepress-plugin-seo": "^0.2.0"
