{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"name": "docs",
	"projectType": "application",
	"sourceRoot": "apps/docs",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["dist/apps/docs"],
			"options": {
				"command": "vuepress check-md apps/docs && vuepress build apps/docs --dest dist/apps/docs"
			}
		},
		"serve": {
			"executor": "nx:run-commands",
			"outputs": ["apps/docs/.vuepress/.temp", "apps/docs/.vuepress/.cache"],
			"options": {
				"command": "vuepress check-md apps/docs && vuepress dev apps/docs"
			}
		}
	},
	"tags": []
}
